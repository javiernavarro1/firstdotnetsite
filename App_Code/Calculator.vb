Imports Microsoft.VisualBasic

Public Class Calculator
    Public Function Add(ByVal sampleA As Double, ByVal sampleB As Double) As Double
        Return sampleA + sampleB
    End Function
    Public Function Subtract(ByVal sampleA As Double, ByVal sampleB As Double) As Double
        Return sampleA - sampleB
    End Function
    Public Function Multiply(ByVal sampleA As Double, ByVal sampleB As Double) As Double
        Return sampleA * sampleB
    End Function
    Public Function Divide(ByVal sampleA As Double, ByVal sampleB As Double) As Double
        Return sampleA / sampleB
    End Function
End Class
